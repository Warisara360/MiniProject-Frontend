n            this.lastName = item.lastName\r\n            this.salary = item.salary\r\n            this.roles = item.role.name\r\n            this.idEmployee = item.id\r\n          }\r\n        },\r\n    \r\n        editItem (item) {\r\n          console.log('item select', item)\r\n          this.editedIndex = this.employeeItem.indexOf(item)\r\n          this.editedItem = Object.assign({}, item)\r\n          this.dialog = true\r\n        },\r\n    \r\n        deleteItem (item) {\r\n          this.idforDelete = item.id\r\n          this.dialogDelete = true\r\n        },\r\n    \r\n        async deleteItemConfirm () {\r\n          try {\r\n            var response = await this.axios.delete('http://localhost:8000/menu/' + this.idforDelete)\r\n            this.initialize()\r\n          } catch (error) {\r\n            console.log(error.message)\r\n          }\r\n          this.closeDelete()\r\n        },\r\n    \r\n        close () {\r\n          this.dialogCreate = false\r\n          this.editedItem = []\r\n          this.editedIndex = -1\r\n          this.defaultItem = {\r\n            name: '',\r\n            calories: 0,\r\n            fat: 0,\r\n            carbs: 0,\r\n            protein: 0\r\n          }\r\n        },\r\n    \r\n        closeDelete () {\r\n          this.dialogDelete = false\r\n          this.$nextTick(() => {\r\n            this.editedItem = Object.assign({}, this.defaultItem)\r\n            this.editedIndex = -1\r\n          })\r\n        },\r\n    \r\n        async save (action) {\r\n          var data = {\r\n            fristName: this.fristName,\r\n            lastName: this.lastName,\r\n            salary: this.salary,\r\n            role: {\r\n              name: this.roles\r\n            },\r\n            skills: [\r\n              { skill: '' }\r\n            ]\r\n          }\r\n          if (action === 'เพิ่มข้อมูล') {\r\n            try {\r\n              var dataResponse = await this.axios.post('http://localhost:8000/menu', data)\r\n              console.log('dataResponse ====>', dataResponse)\r\n              this.close()\r\n              this.initialize()\r\n            } catch (error) {\r\n              console.log(error.message)\r\n            }\r\n          } else {\r\n            try {\r\n              var dataResponse = await this.axios.put('http://localhost:8000/menu/' + this.idEmployee, data)\r\n              console.log('dataResponse ====>', dataResponse)\r\n              this.close()\r\n              this.initialize()\r\n            } catch (error) {\r\n              console.log(error.message)\r\n            }\r\n          }\r\n        }\r\n      }\r\n    }\r\n    </script>\r\n    \r\n    <style>\r\n    \r\n    </style>"],"mappings":"AAsJA;EACAA,IAAA,EAAAA,CAAA;IACAC,UAAA;IACAC,SAAA;IACAC,QAAA;IACAC,UAAA;IACAC,YAAA;IACAC,YAAA;IACAC,OAAA,GACA;MACAC,IAAA;MACAC,KAAA;MACAC,QAAA;MACAC,KAAA;IACA,GACA;MAAAH,IAAA;MAAAG,KAAA;IAAA,GACA;MAAAH,IAAA;MAAAG,KAAA;IAAA,GACA;MAAAH,IAAA;MAAAG,KAAA;IAAA,GACA;MAAAH,IAAA;MAAAG,KAAA;IAAA,GACA;MAAAH,IAAA;MAAAG,KAAA;MAAAD,QAAA;IAAA,EACA;IACAE,QAAA;IACAC,WAAA;IACAC,UAAA;MACAC,IAAA;MACAC,QAAA;MACAC,GAAA;MACAC,KAAA;MACAC,OAAA;IACA;IACAC,WAAA;MACAL,IAAA;MACAC,QAAA;MACAC,GAAA;MACAC,KAAA;MACAC,OAAA;IACA;IACAE,SAAA;IACAC,MAAA;IACAC,WAAA;EACA;EAEAC,KAAA;IACAC,OAAAC,GAAA;MACAA,GAAA,SAAAC,KAAA;IACA;IACArB,aAAAoB,GAAA;MACAA,GAAA,SAAAE,WAAA;IACA;EACA;EAEAC,QAAA;IACA,KAAAC,UAAA;EACA;EAEAC,OAAA;IACA,MAAAD,WAAA;MACA,KAAAE,YAAA;MACA;QACA,IAAAhC,IAAA,cAAAiC,KAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,oBAAApC,IAAA;QACA,KAAAgC,YAAA,GAAAhC,IAAA,CAAAA,IAAA;MACA,SAAAqC,KAAA,GAEA;IACA;IACAC,WAAAC,MAAA,EAAAC,IAAA;MACA,KAAAnB,SAAA;MACA,IAAAkB,MAAA;QACA,KAAAlC,YAAA;QACA,KAAAgB,SAAA;QACA,KAAAP,UAAA,GAAA0B,IAAA;MACA;QACA,KAAAnB,SAAA;QACA,KAAAhB,YAAA;QACA,KAAAoC,SAAA,GAAAD,IAAA,CAAAC,SAAA;QACA,KAAAC,QAAA,GAAAF,IAAA,CAAAE,QAAA;QACA,KAAAC,MAAA,GAAAH,IAAA,CAAAG,MAAA;QACA,KAAAC,KAAA,GAAAJ,IAAA,CAAAK,IAAA,CAAA9B,IAAA;QACA,KAAA+B,UAAA,GAAAN,IAAA,CAAAO,EAAA;MACA;IACA;IAEAC,SAAAR,IAAA;MACAL,OAAA,CAAA