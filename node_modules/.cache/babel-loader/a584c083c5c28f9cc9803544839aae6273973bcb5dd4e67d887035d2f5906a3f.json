er\">\r\n                      <h1>\r\n                        {{ n.nameMenu }}\r\n                      </h1>\r\n                    </div>\r\n\r\n                    <div style=\"padding-top: 5px\" >\r\n                      <v-card class=\"overflow-x-auto  lighten-5\" :elevation=\"0\" style=\"width: 500px; height: 150px; border-radius: 20px\">\r\n                      <v-card-title >\r\n                        <h6 class=\"indent2\" style=\"font-size: 17px\">{{ n.detailMenu }}</h6>\r\n                      </v-card-title>\r\n                    </v-card>\r\n                    </div>\r\n                    <div style=\"padding-top: 5px; display: flex; justify-content: center;\" >\r\n                      <v-card-title>\r\n                        <p style=\"font-size: 17px;  color: rgb(129, 148, 148);\">คลิกเพื่ออ่านเพิ่มเติม</p>\r\n                      </v-card-title>\r\n                    </div>\r\n                  </div>\r\n                 \r\n                </v-card>\r\n              </v-hover>\r\n            </v-col>\r\n          </v-row>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n<v-dialog v-model=\"dialogVisible\" max-width=\"500\" >\r\n    <template>\r\n      <v-card width=\"500\" height=\"500\" class=\"overflow-auto\">\r\n        \r\n        <p>{{ selectedItem ? selectedItem.nameMenu  : '' }}</p>\r\n        <p>{{ selectedItem ? selectedItem.detailMenu  : '' }}</p>\r\n        <p>{{ selectedItem ? selectedItem.ingerMenu  : '' }}</p>\r\n        <p>{{ selectedItem ? selectedItem.gastroMenu  : '' }}</p>\r\n      </v-card>\r\n    </template>\r\n  </v-dialog>\r\n   \r\n\r\n  \r\n  </div>\r\n</template>\r\n\r\n\r\n\r\n<script>\r\nimport Swal from \"sweetalert2\";\r\nimport axios from \"axios\";\r\nimport AppbarUser from \"./AppbarUser.vue\";\r\nexport default {\r\n  components: {\r\n    AppbarUser,\r\n  },\r\n  data() {\r\n    return {\r\n      desserts: [],\r\n      dialogVisible: false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getMemuData(