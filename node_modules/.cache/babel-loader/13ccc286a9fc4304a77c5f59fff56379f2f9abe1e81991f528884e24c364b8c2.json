nt>\r\n                        <v-list-item-title\r\n                          v-text=\"user.galleryname\"\r\n                        ></v-list-item-title>\r\n                      </v-list-item-content>\r\n                      <v-spacer></v-spacer>\r\n                      <v-icon>mdi-comment-outline</v-icon>\r\n                      <div>comment</div>\r\n                    </v-card-actions>\r\n                  </v-card>\r\n                </template>\r\n\r\n                <template>\r\n                  <v-card>\r\n                    <v-toolbar color=\"#893744\" dark>\r\n                      <v-list-item-avatar color=\"grey darken-3\">\r\n                        <v-img\r\n                          class=\"elevation-6\"\r\n                          alt\r\n                          :src=\"'data:image/jpeg;base64,' + user.picprofile\"\r\n                        ></v-img>\r\n                      </v-list-item-avatar>\r\n                      <v-list-item-content>\r\n                        <v-list-item-title\r\n                          v-text=\"user.galleryname\"\r\n                        ></v-list-item-title>\r\n                      </v-list-item-content>\r\n                      <v-spacer></v-spacer>\r\n                    </v-toolbar>\r\n\r\n                    <v-card-text>\r\n                      <v-card>\r\n                        <v-img\r\n                          style=\"\r\n                            height: 100%;\r\n                            width: 100%;\r\n                            margin-top: 10px;\r\n                            border-radius: 8px;\r\n                          \"\r\n                          :src=\"'data:image/jpeg;base64,' + item.pic\"\r\n                        />\r\n                        <v-card-title v-text=\"item.name\"></v-card-title>\r\n                        <v-card-text v-text=\"item.caption\"></v-card-text>\r\n                        <v-card-subtitle\r\n                          v-text=\"'Posted: ' + formatted